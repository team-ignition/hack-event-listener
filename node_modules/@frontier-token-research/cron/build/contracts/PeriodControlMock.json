  "source": "pragma solidity ^0.4.24;\nimport \"../calendar/Daily.sol\";\n\n\n/**\n * Contract code based on pipemerrian's Date Time Smart contract, now available as a library.\n * https://github.com/pipermerriam/ethereum-datetime/blob/master/contracts/DateTime.sol\n * Provide Datetime utils on Ethereum Smart Contracts\n */\n\ncontract PeriodControlMock is Daily {\n\t\n\tuint256 currentPeriod = 0;\n\n\tfunction mock_next() public {\n\t\tcurrentPeriod++;\n\t}\n\n\tfunction mock_next(uint256 _periodsToAdvance) public {\n\t\tcurrentPeriod = currentPeriod+_periodsToAdvance;\n\t}\n\n\tfunction height() public view returns (uint256){\n\t\treturn currentPeriod;\n\t}\n\n}\n",
